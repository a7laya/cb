<view class="page page-fill"><view class="header"><block wx:if="{{userIsLogin}}"><view class="info-wapper"><view class="nickname">{{''+userInfo.username+''}}</view></view></block><block wx:else><view><navigator url="../login/login"><view class="nickname regist-login">登录</view></navigator></view></block></view><view class="page-block info-list"><view data-event-opts="{{[['tap',[['changePassword',['$event']]]]]}}" class="item-wapper " bindtap="__e"><view class="info-words">更改密码</view><view class="right-wapper"><view class="arrow-block"><image class="arrow-ico" src="../../static/icons/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view><view data-event-opts="{{[['tap',[['timerSet',['$event']]]]]}}" class="item-wapper" bindtap="__e"><view class="info-words">定时设置</view><view class="right-wapper"><view class="arrow-block"><image class="arrow-ico" src="../../static/icons/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view></view><view class="footer-wapper"><view data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="footer-words" style="margin-top:10rpx;" bindtap="__e">退出登录</view></view></view>