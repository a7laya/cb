<view class="page page-block"><view class="search-block"><view class="uni-list"><view class="uni-list-cell"><view class="uni-list-cell-db"><picker value="{{indexYear}}" range="{{arrayYear}}" data-event-opts="{{[['change',[['bindPickerChangeYear',['$event']]]]]}}" bindchange="__e"><view class="uni-input-year">{{arrayYear[indexYear]}}</view></picker></view><view class="uni-list-cell-left">年</view></view></view><view class="uni-list"><view class="uni-list-cell"><view class="uni-list-cell-db"><picker class="uni-input-block" value="{{indexMonth}}" range="{{arrayMonth}}" data-event-opts="{{[['change',[['bindPickerChangeMonth',['$event']]]]]}}" bindchange="__e"><view class="uni-input-month">{{arrayMonth[indexMonth]}}</view></picker></view><view class="uni-list-cell-left">月</view></view></view><input class="search-text" placeholder="搜索ID|位置" maxlength="10" confirm-type="search" data-event-opts="{{[['confirm',[['searchMe',['$event']]]],['input',[['__set_model',['','keywords','$event',[]]]]]]}}" value="{{keywords}}" bindconfirm="__e" bindinput="__e"/><view class="search-ico-wapper"><image class="search-ico" src="../../static/icons/search.png" data-event-opts="{{[['tap',[['searchMe',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="vcs-name"><block wx:for="{{vcsList}}" wx:for-item="vcs" wx:for-index="index" wx:key="index"><view><block wx:if="{{vcs.istheMonthFirstItem}}"><view>{{''+vcs.year+"."+vcs.month+''}}</view></block><uni-collapse bind:change="__e" vue-id="{{'1-'+index}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><uni-collapse-item class="vue-ref-in-for" vue-id="{{('2-'+index)+','+('1-'+index)}}" title="{{' '+vcs.meter_id+(' '+vcs.location)}}" show-animation="{{true}}" data-ref="add" bind:__l="__l" vue-slots="{{['default']}}"><view class="data-block"><view class="title"><text class="title-text">月份</text></view><view class="value"><text class="value-text">{{vcs.year+"-"+vcs.month}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">水表ID</text></view><view class="value"><text class="value-text">{{vcs.meter_id}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">正累积流量</text></view><view class="value"><text class="value-text">{{vcs.zljll}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">反累积流量</text></view><view class="value"><text class="value-text">{{vcs.fljll}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">正累积热量</text></view><view class="value"><text class="value-text">{{vcs.zljrl}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">反累积热量</text></view><view class="value"><text class="value-text">{{vcs.fljrl}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">净累积流量</text></view><view class="value"><text class="value-text">{{vcs.jljll}}</text></view></view><view class="data-block"><view class="title"><text class="title-text">净累积热量</text></view><view class="value"><text class="value-text">{{vcs.jljrl}}</text></view></view></uni-collapse-item></uni-collapse></view></block></view></view>